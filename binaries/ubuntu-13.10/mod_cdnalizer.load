<IfModule mod_filter.c>
        LoadModule cdnalizer_module /usr/lib/apache2/modules/mod_cdnalizer.so
        FilterDeclare cdnalizer CONTENT_SET
        FilterProtocol cdnalizer change=yes;byteranges=yes
        FilterProvider cdnalizer CDNALIZER "(%{CONTENT_TYPE} =~ /xml/i) || (%{CONTENT_TYPE} =~ /html/i)"
        FilterChain +cdnalizer
</IfModule>

